{
  "version": 2,
  "name": "trenki",
  "scope": "eskiimos",
  "builds": [
    {
      "src": "telegram-workout-app/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "telegram-workout-app/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "telegram-workout-app/src/app/api/telegram/route.ts": {
      "maxDuration": 10
    }
  }
}
